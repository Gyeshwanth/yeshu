<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  

<bean id ="std" class ="com.test.Student" autowire="byName">
<property name="id" value ="101"></property>
<property name="name" value ="ram"></property>
<property name="college" value ="svt"></property>

</bean>
<bean id ="address" class ="com.test.Address">
<property name="city" value="mpl"></property>
<property name="dist" value="ap"></property>
<property name="state" value ="chittoor"></property>
</bean>



</beans>